<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFFFF">

    <!-- Line Divider -->
    <View
        android:id="@+id/line_divider"
        android:layout_width="368dp"
        android:layout_height="1dp"
        android:layout_marginTop="935dp"
        android:layout_marginStart="3dp"
        android:background="#808080"
        android:layout_alignParentTop="true" />

    <!-- Rectangle 70 -->
    <ImageView
        android:id="@+id/rectangle_70"
        android:layout_width="316dp"
        android:layout_height="69dp"
        android:layout_marginTop="703dp"
        android:layout_marginStart="29dp"
        android:src="@drawable/image1_52324"
        android:scaleType="fitXY"
        android:layout_alignParentTop="true" />

    <!-- Back Text -->
    <TextView
        android:id="@+id/back_text"
        android:layout_width="183dp"
        android:layout_height="30dp"
        android:layout_marginTop="723dp"
        android:layout_marginStart="163dp"
        android:text="Back"
        android:textSize="25sp"
        android:textColor="#FFFFFFFF"
        android:fontFamily="sans-serif"
        android:gravity="start"
        android:ellipsize="end"
        android:maxLines="1" />

    <!-- Group 6 -->
    <ImageView
        android:id="@+id/group_6"
        android:layout_width="326dp"
        android:layout_height="133dp"
        android:layout_marginTop="172dp"
        android:layout_marginStart="24dp"
        android:src="@drawable/image2_54773"
        android:scaleType="fitXY"
        android:layout_alignParentTop="true" />

    <!-- Group 7 -->
    <ImageView
        android:id="@+id/group_7"
        android:layout_width="326dp"
        android:layout_height="133dp"
        android:layout_marginTop="339dp"
        android:layout_marginStart="25dp"
        android:src="@drawable/image3_54774"
        android:scaleType="fitXY"
        android:layout_alignParentTop="true" />

    <!-- Group 8 -->
    <ImageView
        android:id="@+id/group_8"
        android:layout_width="326dp"
        android:layout_height="133dp"
        android:layout_marginTop="533dp"
        android:layout_marginStart="25dp"
        android:src="@drawable/image4_54775"
        android:scaleType="fitXY"
        android:layout_alignParentTop="true" />

    <!-- Group 4 -->
    <ImageView
        android:id="@+id/group_4"
        android:layout_width="411dp"
        android:layout_height="45dp"
        android:layout_marginTop="27dp"
        android:layout_marginStart="-25dp"
        android:src="@drawable/image5_54723"
        android:scaleType="fitXY"
        android:layout_alignParentTop="true" />

    <!-- Image with Shadow -->
    <ImageView
        android:id="@+id/image_shadow"
        android:layout_width="55dp"
        android:layout_height="38dp"
        android:layout_marginTop="31dp"
        android:layout_marginStart="5dp"
        android:src="@drawable/image6_54739"
        android:scaleType="fitXY"
        android:background="@drawable/shadow_background"
        android:layout_alignParentTop="true" />

    <!-- Ranking Text -->
    <TextView
        android:id="@+id/ranking_text"
        android:layout_width="209dp"
        android:layout_height="24dp"
        android:layout_marginTop="100dp"
        android:layout_marginStart="117dp"
        android:text="Ranking"
        android:textSize="32sp"
        android:textColor="#8F8E8E"
        android:fontFamily="sans-serif"
        android:gravity="start"
        android:ellipsize="end"
        android:maxLines="1" />

</RelativeLayout>
